[DEFAULT]
gc = git gc "$@"
sync = git sync-upstream-master
fork = hub fork --remote-name origin

[Code/steshaw/shelly]
checkout = git clone git@github.com:steshaw/shelly

[.emacs.d.doom]
checkout = git clone 'https://github.com/hlissner/doom-emacs' '.emacs.d.doom'

[.fzf]
checkout = git clone 'https://github.com/junegunn/fzf.git' '.fzf'
