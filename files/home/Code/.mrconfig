[idris-lang/Idris2]
checkout = github-clone-fork idris-lang/Idris2
update = mr sync

[NixOS/nix]
checkout = github-clone-fork NixOS/nix
update = mr sync

[NixOS/nixpkgs]
checkout = github-clone-fork NixOS/nixpkgs
update = mr sync

[ghc/ghc]
checkout = 
	git clone --origin upstream --recurse-submodules https://gitlab.haskell.org/ghc/ghc.git
	cd ghc
	git config submodule.recurse true
	git remote add origin https://gitlab.haskell.org/steshaw/ghc.git

[ghc/ghc.nix]
checkout = git clone https://github.com/alpmestan/ghc.nix

