#!/usr/bin/env bash

set -eu

function printPath {
  tr : '\n'
}

echo PATH:
echo "$PATH" | printPath
if [[ ${LD_LIBRARY_PATH:-unset} != unset ]]; then
  echo
  echo LD_LIBRARY_PATH:
  echo "$LD_LIBRARY_PATH" | printPath
fi
if [[ ${DYLD_LIBRARY_PATH:-unset} != unset ]]; then
  echo
  echo DYLD_LIBRARY_PATH:
  echo "$DYLD_LIBRARY_PATH" | printPath
fi
