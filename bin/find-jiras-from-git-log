#!/bin/bash

if [[ $# -ne 2 ]]; then
  echo "usage: $(basename $0) start-commit end-commit" >&2
  exit 2
fi

startCommit=$1
endCommit=$2

# The '^' is appended the bottom/since hash because is not inclusive.
git log "${startCommit}^..${endCommit}" | grep EL- | perl -pe 's/^.*\b(EL-\d+)\b.*$/\1/;' | sort -u
